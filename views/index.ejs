    <div class="hero-content text-center">

      <div class="card w-96 bg-base-100 shadow-xl">
        <figure class="px-10 pt-10">
          <img src="/imgs/llama-celebrate.svg" alt="Llama celebrating" class="rounded-xl three-cards" />
